<mapper namespace="SqlPartnerMapper">
	<resultMap type="Partner" id="PartnerResult">
		<result property="PartnerId" column="Partner_ID"></result>
		<result property="PartnerName" column="Partner_NAME"/>
		<result property="useYn" column="USE_YN"/>
		<result property="createDate"column="CREATE_DATE"/>
		<result property="upateDate"column="UPDATE_DATE"/>
	</resultMap>
	<select id="selectPartnerList" parameterType="Partner" resultMap="PartnerResult">
		<![CDATA[
		SELECT * FROM Partner
		WHERE USE_YN = 'Y'
		]]>
	</select>
</mapper>